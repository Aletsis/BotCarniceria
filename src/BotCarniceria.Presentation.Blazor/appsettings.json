{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=BotCarniceriaDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Hangfire": {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=BotCarniceriaDb;Trusted_Connection=True;MultipleActiveResultSets=true",
    "SchemaName": "hangfire",
    "WorkerCount": 5,
    "EnableDashboard": true,
    "DashboardPath": "/hangfire",
    "Retry": {
      "MaxAttempts": 5,
      "InitialDelaySeconds": 10,
      "BackoffMultiplier": 2.0,
      "MaxDelaySeconds": 3600
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore.SignalR": "Debug",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/blazor-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}